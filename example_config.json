{
  "deepseek": {
    "api_key": "YOUR_DEEPSEEK_API_KEY_HERE",
    "base_url": "https://api.deepseek.com",
    "model": "deepseek-chat",
    "max_tokens": 4096,
    "temperature": 0.7,
    "timeout": 60.0
  },
  "mcp_servers": [
    {
      "name": "filesystem",
      "command": ["python", "-m", "mcp_server_filesystem"],
      "args": ["--root", "."],
      "timeout": 30.0
    }
  ],
  "logging": {
    "level": "INFO",
    "file": null
  },
  "max_concurrent_requests": 5
}